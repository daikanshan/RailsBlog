<p id="notice"></p>


<table class="table table-hover">
  <thead>
    <tr>
      <th>类型名</th>
      <th>图标</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @manage_article_types.each do |manage_article_type| %>
      <tr>
        <td><%= manage_article_type.name %></td>
        <td><span class="glyphicon glyphicon-<%= manage_article_type.glyphicon %>"></span></td>
        <!--<td><%= image_tag manage_article_type.img_url(:thumb) if manage_article_type.img %></td>-->
        <td><%= link_to '编辑', edit_manage_article_type_path(manage_article_type) %></td>
        <td><%= link_to '删除', manage_article_type_path(manage_article_type), method: :delete, data: { confirm: '确定删除?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
