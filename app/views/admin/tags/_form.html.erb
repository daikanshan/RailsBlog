<%= form_for(@admin_tag) do |f| %>
    <%= form_errors(@admin_tag) %>

    <div class="form-group">
      <%= f.text_field :name, class:"form-control",placeholder:"请输入标签名" %>
    </div>

    <div class="form-group action">
      <%= link_to '取消', admin_tags_path,class:"btn btn-default cancle" %>
      <%= f.submit class:"btn btn-primary submit" %>
    </div>
<% end %>
